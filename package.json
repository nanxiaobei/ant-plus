{
  "name": "antx",
  "version": "0.0.1",
  "description": "Enhanced Ant Design Form",
  "keywords": [
    "react",
    "ant",
    "antd",
    "ant-design",
    "ant-plus"
  ],
  "license": "MIT",
  "author": "nanxiaobei <nanxiaobei@gmail.com> (https://github.com/nanxiaobei)",
  "homepage": "https://github.com/nanxiaobei/ant-plus",
  "repository": "github:nanxiaobei/ant-plus",
  "bugs": "https://github.com/nanxiaobei/ant-plus/issues",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist lib es",
    "build:commonjs": "cross-env BABEL_ENV=cjs NODE_ENV=cjs rollup -c -o lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/index.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/ant-plus.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/ant-plus.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "dependencies": {
    "antd": "^3.6.2",
    "react": "^16.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sass": "^0.8.0",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
